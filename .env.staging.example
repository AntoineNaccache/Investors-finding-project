# Staging Environment Configuration
# Copy this file to .env.staging and update with your staging values

# === LLM Provider API Keys ===
GROQ_API_KEY=your_groq_api_key_here
GROQ_BASE_URL=https://api.groq.com/openai/v1

# Optional LLM providers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GOOGLE_GENERATIVE_AI_API_KEY=

# === Database ===
POSTGRES_URL=postgres://staging_user:staging_password@staging-db.example.com:5432/investors_staging

# === Authentication ===
BETTER_AUTH_SECRET=generate_with_openssl_rand_base64_32
BETTER_AUTH_URL=https://staging.yourdomain.com

# === Tools ===
EXA_API_KEY=

# === E2B Sandbox (for secure data processing) ===
E2B_API_KEY=

# === Environment ===
NODE_ENV=staging
NO_HTTPS=1

# === Optional: Redis ===
REDIS_URL=

# === Optional: OAuth ===
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# === Optional: File Storage ===
# FILE_STORAGE_TYPE=s3
# FILE_STORAGE_S3_BUCKET=
# FILE_STORAGE_S3_REGION=
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
